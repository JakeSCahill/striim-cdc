<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.4">

    <entity name="CompoundProductInv" class="com.hazelcast.cdc.pojo.ProductInv" >
        <table name="ORCLPDB1.STRIIM.COMPOUND_PRODUCT_INV"/>
        <id-class class="com.hazelcast.cdc.pojo.ProductKey"/>
        <attributes>
            <id name ="sku" attribute-type="Long"  >
                <column nullable="false" name="SKU" />
            </id>
            <id name ="skuHash" attribute-type="String"  >
                <column nullable="false" name="SKU_HASH" />
            </id>
            <basic name="stock" attribute-type="double" >
                <column nullable="false" name="STOCK"  />
            </basic>
            <basic name="name" attribute-type="String" >
                <column  name="NAME" />
            </basic>
            <basic name="lastUpdated"  attribute-type="java.util.Date" >
                <column name="LAST_UPDATED" />
            </basic>
        </attributes>
    </entity>
</entity-mappings>